USE IN CONSOLE

(() => {
  const domainRegex = /([a-z0-9][a-z0-9\-]*\.)+[a-z]{2,}/gi;
  const matches = Array.from(document.body.innerText.matchAll(domainRegex)).map(m => m[0].toLowerCase());
  const unique = [...new Set(matches)];
  unique.forEach(d => console.log(d));
})();
